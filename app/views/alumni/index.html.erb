<p style="color: green"><%= notice %></p>

<% content_for :title, "Alumni" %>

<h1>You're logged in! Welcome to the Dashboard!</h1>
<%= link_to "Sign Out", destroy_user_session_path %>

<h1>Alumni Page</h1>

<div id="alumni">
  <% @alumni.each do |alumnus| %>
    <%= render alumnus %>
    <p>
      <%= link_to "Show this alumnus", alumnus %>
    </p>
  <% end %>
</div>

<%= link_to "New alumnus", new_alumnus_path %>
